{
  "name": "react-context-utils",
  "version": "1.1.0",
  "description": "Simple component and decorator to provide contexts to React components",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "lint": "eslint src tests",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register tests/index.js",
    "test:watch": "npm test -- --watch",
    "bundle": "NODE_ENV=production webpack --colors --progress",
    "lib": "babel src --out-dir lib",
    "build": "npm run test && npm run lint && npm run lib && npm run bundle"
  },
  "author": "Mathieu ANCELIN",
  "license": "MIT",
  "keywords": [
    "react",
    "context",
    "utils",
    "DI",
    "higher order component"
  ],
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "jsdom": "^8.5.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.0.2",
    "webpack": "^1.13.0"
  }
}
