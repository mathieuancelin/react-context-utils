{
  "name": "react-context-utils",
  "version": "1.0.5",
  "description": "Simple component and decorator to provide contexts to React components",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "lint": "eslint src tests",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register tests/index.js",
    "test:watch": "npm test -- --watch",
    "bundle": "NODE_ENV=production webpack -p --colors --progress",
    "lib": "babel src --out-dir lib",
    "build": "npm run test && npm run lint && npm run lib && npm run bundle"
  },
  "author": "Mathieu ANCELIN",
  "license": "MIT",
  "keywords": [
    "react",
    "context",
    "utils",
    "DI",
    "higher order component"
  ],
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.16.1",
    "jsdom": "^8.0.2",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.13"
  }
}
